<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tp1_cm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tp1_CM">
        <activity android:name=".MapActivity"></activity>
        <activity
            android:name=".EditNotasActivity"
            android:label="@string/editnota"
            android:parentActivityName=".NotasActivity" />
        <activity
            android:name=".AddNotasActivity"
            android:label="@string/title_addNota"
            android:parentActivityName=".NotasActivity" />
        <activity
            android:name=".NotasActivity"
            android:label="@string/title_Nota"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".RegistoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>